{
    "env": {
        "es6": false,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-undef":"error",
        "no-unused-vars": "error",
        "no-console": "off"
    },
    "globals": {
        "console": true
    }
}
